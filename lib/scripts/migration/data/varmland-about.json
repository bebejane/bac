  {
    "id": 121,
    "date": "2015-06-24T20:39:08",
    "date_gmt": "2015-06-24T18:39:08",
    "guid": {
      "rendered": "http://varmland.konstframjandet.se/?post_type=kf-about&#038;p=121"
